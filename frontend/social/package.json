{
  "name": "social",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "rimraf dist && webpack --bail --progress --profile --docs --env dev",
    "build:prod": "rimraf dist && webpack --bail --progress --profile --docs --env prod",
    "start": "npm-run-all --parallel serve",
    "serve": "webpack-dev-server --history-api-fallback --inline --progress --hot --env dev",
    "serve:prod": "webpack-dev-server --history-api-fallback --inline --progress --hot --env prod",
    "test": "npm run lint && karma start",
    "test-watch": "karma start --auto-watch --no-single-run",
    "lint": "eslint src/ && htmlhint src/ && csslint src/styles/"
  },
  "dependencies": {
    "angular": "^1.6.0",
    "angular-aria": "^1.6.0",
    "angular-route": "^1.8.3",
    "angular-sanitize": "^1.8.3",
    "angular-ui-router": "^1.0.0-beta.3",
    "config": "^3.3.9",
    "express": "^4.18.2",
    "fb": "^2.0.0",
    "http": "^0.0.1-security",
    "moment": "^2.29.4",
    "ng-sanitize": "^0.0.2",
    "request-promise": "^4.2.6",
    "restangular": "^1.5.2"
  },
  "devDependencies": {
    "angular-mocks": "^1.6.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-preset-es2015": "^6.16.0",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.25.0",
    "csslint": "^1.0.4",
    "eslint": "^3.9.1",
    "eslint-config-angular": "^0.5.0",
    "eslint-plugin-angular": "^1.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.7.1",
    "htmlhint": "^0.9.13",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.2.0",
    "ng-annotate-loader": "^0.2.0",
    "npm-run-all": "^3.1.2",
    "postcss-loader": "^1.1.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-angular-dgeni-plugin": "^0.0.1",
    "webpack-dev-server": "^1.16.1",
    "webpack-merge": "^0.14.1"
  }
}
